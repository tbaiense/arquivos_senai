package japedidos.cliente;

/**
 * @author t.baiense
 */
public class ClientePJ extends Cliente {
	String cnpj;
	String nomeFantasia, razaoSocial;
	
	public ClientePJ(int id, String nomeFant, String razaoSocial, String cnpj) {
		super(id, TipoCliente.PJ);
		this.cnpj = cnpj;
		this.nomeFant = nomeFant;
		this.razaoSoc = razaoSoc;
		this.cnpj = cnpj;
	};
}
